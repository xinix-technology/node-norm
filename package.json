{
  "name": "node-norm",
  "version": "1.2.20",
  "description": "Norm is Not ORM though data access layer",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "test": "npm run test:node && npm run test:browser",
    "lint": "eslint .",
    "coverage": "nyc --reporter=lcov npm run test:node",
    "build": "webpack-cli --mode=development && webpack-cli --mode=production",
    "stat": "webpack-cli --mode production --json > stat.json",
    "test:node": "mocha test/node/**",
    "test:browser": "karma start"
  },
  "author": "Ganesha <reekoheek@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xinix-technology/node-norm.git"
  },
  "dependencies": {
    "async-factory": "^1.0.0",
    "big.js": "^5.2.2",
    "fs-extra": "^7.0.1",
    "generic-pool": "^3.4.2",
    "koa-compose": "^4.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
